{
  "manifest_version": 3,
  "name": "Email Co-Pilot",
  "version": "1.0.0",
  "description": "Automate your email",
  "key": "",
  "oauth2": {
    "client_id": "828198973510-0f3v8hn4fvd2aaneh9ugsuqnpjbav7uk.apps.googleusercontent.com",
    "scopes":["https://www.googleapis.com/auth/gmail.readonly"]
  },
  "action": {
    "default_icon": "assets/icon.png",
    "default_popup": "index.html"
  },
  "icons": {
    "16": "assets/icon16.png",
    "48": "assets/icon48.png",
    "128": "assets/icon128.png"
  },
  "permissions": [
    "tabs",
    "identity",
    "identity.email",
    "storage"
  ],
  "background": {
    "service_worker": "background.js"
  }
}